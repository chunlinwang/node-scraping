{
    "index_patterns": ["node-scraping-app-*"],
      "template": {
      "settings": {
          "number_of_shards": 1,
          "index.lifecycle.name": "ecom_ilm_policy",
          "index.lifecycle.rollover_alias":"ecom"
        },
        "mappings": {
          "properties": {
            "title": {
              "type": "keyword"
            },
            "brand": {
              "type": "keyword"
            },
            "url": {
              "type": "keyword" 
            },
            "productName": {
              "type": "keyword" 
            },
            "originPrice": {
                "type": "double"
            },
            "salePrice": {
                "type": "double"
            },
            "currency": {
                "type": "text"
            }
          }
        }
      },
      "priority": 51,
      "version": 1,
      "_meta": {
        "description": "my custom"
      }
    }